# From https://docs.github.com/en/actions/learn-github-actions/essential-features-of-github-actions#sharing-data-between-jobs
name: download-artifact-test
run-name: ${{ github.actor }} is learning download artifact GitHub Actions
on: [push]
jobs:
  download-artifact-job:
    name: Download artifact
    runs-on: ubuntu-latest
    steps:
      - name: Download a single artifact
        uses: actions/download-artifact@v3
        with:
          name: output-log-file
